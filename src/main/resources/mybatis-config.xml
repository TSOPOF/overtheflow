<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Aliasing -->
	<typeAliases>
		<typeAlias alias="MemberClassDTO" type="com.lecto.tsop.dto.MemberClassDTO"/>
		<typeAlias alias="MemberDTO" type="com.lecto.tsop.dto.MemberDTO"/>
		<typeAlias alias="FolderDTO" type="com.lecto.tsop.dto.FolderDTO"/>
		<typeAlias alias="FileDTO" type="com.lecto.tsop.dto.FileDTO"/>
		<typeAlias alias="BooleanCharTypeHandler" type="com.lecto.tsop.typehandler.BooleanCharTypeHandler"/>
	</typeAliases>
	 
	<typeHandlers>
		<typeHandler handler="com.lecto.tsop.typehandler.BooleanCharTypeHandler" javaType="boolean" jdbcType="CHAR"/>
	</typeHandlers>
</configuration>